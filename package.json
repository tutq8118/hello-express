{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "hello-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "startn": "nodemon server.js",
    "build-css": "node-sass --include-path scss scss/custom.scss public/custom.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "npm-run-all --parallel watch-css startn"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.1.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.9.19",
    "multer": "^1.4.2",
    "pug": "^2.0.4",
    "shortid": "^2.2.15"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  }
}
